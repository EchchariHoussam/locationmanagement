# CarMarket Pro - Variables d'environnement (copier vers .env)

# PostgreSQL (pour run local des services)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Par service (override si besoin)
# auth
POSTGRES_DB=auth_db
JWT_ACCESS_SECRET=your-256-bit-access-secret-key-for-carmarket-pro-auth
JWT_REFRESH_SECRET=your-256-bit-refresh-secret-key-for-carmarket-pro

# Eureka
EUREKA_URI=http://localhost:8761/eureka/

